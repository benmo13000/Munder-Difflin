<%- include('../partials/header') %>

<h2>QUOTES MUTHUFUCKA</h2>
<table>
  <thead>
    <tr>
      <th>Actor</th>
      <th>Quote</th>
    </tr>
  </thead>
  <tbody>
    <!-- Write the line of EJS to iterate over movies using forEach -->
    <% quotes.forEach(function(q) { %>
      <tr>
        <td><%= q.actor %></td>
        <td><%= q.quote %></td>
      </tr>
    <!-- Close the forEach using EJS here -->
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>